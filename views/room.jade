extends layout
block content

	div(class='row')
		div(class='col s6')
			h4#roomIdHeading Room Name: 
				span#roomId= roomId
			p Winner: 
				span#firstClicked N/A
			span#displayName= displayName

		div(class='col s6')
			h4#userListHeading User List
			ul#userList
			div(class='row')
				div(class='col s12')
					button(id='answer', class='waves-effect waves-light btn') Buzz In
				div(class='col s12')
					button(id='reset', class='waves-effect waves-light btn') Reset


	script(src='/js/app.js')